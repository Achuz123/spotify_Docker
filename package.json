{
  "name": "spotify-playlist-keyword-filter",
  "version": "0.0.1",
  "description": "A web app to filter song with certain keywords from Spotify playlists.",
  "main": "server.js",
  "dependencies": {
    "@hapi/boom": "latest",
    "@hapi/cryptiles": "latest",
    "@hapi/hoek": "latest",
    "axios": "latest",
    "cheerio": "latest",
    "child_process": "^1.0.2",
    "cookie-parser": "latest",
    "cors": "^2.8.5",
    "express": "latest",
    "local-cors-proxy": "latest",
    "proxy-list-random": "latest",
    "public-ip": "latest",
    "querystring": "latest",
    "request": "latest",
    "tunnel": "latest"
  },
  "devDependencies": {
    "eslint": "latest",
    "eslint-config-standard": "latest",
    "npm-run-all": "latest",
    "stylelint": "latest",
    "stylelint-config-standard": "latest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/simwai/spotify-playlist-keyword-filter.git"
  },
  "keywords": [
    "spotify"
  ],
  "author": "Simon Waiblinger",
  "license": "CC BY-NC-SA 4.0",
  "bugs": {
    "url": "https://github.com/simwai/spotify-playlist-keyword-filter/issues"
  },
  "homepage": "https://github.com/simwai/spotify-playlist-keyword-filter#readme",
  "scripts": {
    "start": "npm-run-all -p start-server start-proxy",
    "start-server": "node server.js",
    "start-proxy": "lcp --proxyUrl https://www.azlyrics.com",
    "debug": "npm-run-all -p debug-server start-proxy",
    "debug-server": "node --inspect server.js",
    "stylelint": "npx stylelint **/*.css"
  }
}
